    </formulas>
    <formulas>
        <name>RemoveBrackets</name>
        <dataType>String</dataType>
        <expression>TRIM(SUBSTITUTE(SUBSTITUTE({!MultipleChildRecordId},&quot;[&quot;,&quot;&quot;),&quot;]&quot;,&quot;&quot;)) + &quot;;&quot;</expression>
    </formulas>
    <formulas>
