<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billing_POBox_should_be_numeric</fullName>
    <active>true</active>
    <description>Billing POBox should be numeric</description>
    <errorConditionFormula>AND( NOT( ISNUMBER( QAS_Billing_POBox__c)), NOT(ISBLANK(QAS_Billing_POBox__c)))</errorConditionFormula>
    <errorDisplayField>QAS_Billing_POBox__c</errorDisplayField>
    <errorMessage>Enter numeric value</errorMessage>
</ValidationRule>
