<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LogDate_cannot_be_less_than_RecDate</fullName>
    <active>true</active>
    <description>Log Date cannot be less than Received Date</description>
    <errorConditionFormula>Log_Date__c &lt; Received_Date__c</errorConditionFormula>
    <errorDisplayField>Log_Date__c</errorDisplayField>
    <errorMessage>Log Date cannot be less than Received Date</errorMessage>
</ValidationRule>
