                            $('#FormAlert').show();
                            return false;                    
                        }
                        if (document.getElementById('1').files[0].size > 4000000) {
                            valid = false;
                            $('#FormAlert').html('Maximum allowed size of attachment should be less than 4 MB');
                            $('#FormAlert').show();
                            return false;                     
                        }
                        
                        if(valid) {
                            button.disabled = true;
