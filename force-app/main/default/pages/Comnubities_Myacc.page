        <!-- Submit form popup -->
        <div class="modal fade in" id="submit-form-modal" tabindex="-1" role="dialog" aria-labelledby="modalLabel">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <!-- Modal Header -->
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="resetDefaults()">
                            <span aria-hidden="true">&times;</span>
                        </button>
                        <h4 class="modal-title" id="modal-label"><i class="fa fa-file-text"></i> &nbsp; Name Change Request</h4>
                    </div>
                    
                    <!-- Modal Body -->
                    <div class="modal-body">
                        <div class="col-md-12" style="margin-bottom:20px; border-bottom: 1px solid #cccccc3d; padding-bottom: 10px;">
                            Please provide copy of the license that the new name appears to Bright MLS for the change request to be processed.
                        </div>
                        
                        <!-- Alert to display errors -->
                        <div class="alert alert-danger alert-dismissible" id="FormAlert" style="display:none; clear:both;">
                            This alert box could indicate a dangerous or potentially negative action.
                        </div>                                                               
                        
                        <div class="form-group">
                            <span class="col-sm-4 control-label"><b>First Name</b><span style="color:red;">*</span></span>
                            <div class="col-sm-5">
                                <input type="text" class="form-control" id="FirstNameModal" placeholder="Enter First Name" autocomplete="off" maxlength="249"/>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <span class="col-sm-4 control-label"><b>Middle Name</b></span>
                            <div class="col-sm-5">
                                <input type="text" class="form-control" id="MiddleNameModal" placeholder="Enter Middle Name" autocomplete="off" maxlength="249" />
                            </div>
                        </div> 
                        
                        <div class="form-group">
                            <span class="col-sm-4 control-label"><b>Last Name</b><span style="color:red;">*</span></span>
                            <div class="col-sm-5">
                                <input type="text" class="form-control" id="LastNameModal" placeholder="Enter Last Name" autocomplete="off" maxlength="249" />
                            </div>                                  
                        </div>
                        
                        <div class="col-md-12" style="margin-top:20px; padding-bottom: 10px;">
                            Please attach license copy for the name change request to be processed:
                        </div>
                        
                        <div class="form-group">
                            <span class="col-sm-4 control-label"><b>Add Attachment</b><span style="color:red;">*</span></span>
                            <div class="col-sm-5">
                                <input type="file" id="1" name="file" style="margin-top:5px;" class="filetime"/>
                                <input class="form-control" type="hidden" name="filecount" id="filecount" value="1"/>
                            </div>
                        </div>                                
                    </div>
                    
                    <!-- Modal Footer -->
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-primary" onclick="submitForm(this)" html-data-dismiss="modal">Submit</button>
                    </div>
                </div>
            </div>
        </div>
        
 
        
