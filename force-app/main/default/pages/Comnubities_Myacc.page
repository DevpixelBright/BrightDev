                                preferredFirstName += $("#PN_MiddleInitial").val();
                            }
                            if($("#PN_LastName").val() != 'NONE') {
                                preferredNameTxt += $("#PN_LastName").val() + ' ';                                            
                                preferredLastName += $("#PN_LastName").val() + ' ';
                            }
