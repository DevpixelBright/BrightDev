    <rules>
        <fullName>TeamMemberUpdated</fullName>
        <actions>
            <name>SFDC_TeamMember</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>Team_Members__c.Status_of_Invitation__c</field>
            <operation>equals</operation>
            <value>Accepted</value>
        </criteriaItems>
        <criteriaItems>
            <field>Team_Members__c.Status_of_Invitation__c</field>
            <operation>equals</operation>
            <value>Removed</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
